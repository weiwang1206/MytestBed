package com.testbed;

public interface RouterTableItem {
	
	/**
	 * Returns the ID of the roterTable Item.
	 * @return 
	 */
	public int getId();
	
	/**
	 * Returns the code the the roterTable Item.
	 * @return code
	 */
	public String getCode();
	
	/**
	 * Returns the DestIP the the roterTable Item.
	 * @return destIP
	 */
	public String getDesIP();
	
	
	/**
	 * Returns the NextHop of the routerTable item.
	 * @return nextHop
	 */
	public String getNextHop();
	
	public String getExPort();
	
	/**
	 * Returns the VirtualRouter the RouterTable belongs to. 
	 * @return VirtualRouter
	 */
	public VirtualRouter getVR();
	
	public int getVrID();

	public void setVrID(int vrID);
	
	public void setCode(String code);
	
	public void setDesIP(String desIP);
	public void setExPort(String exPort);
	
	public void setNextHop(String nextHop);
	
	
}
