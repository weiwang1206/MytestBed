/* CSS Document */

/*------------------physical machine list-----------------------------------*/



.box_top , .box {
	float:left;
	width: 700px;
	height:250px
	padding: 15px 0 10px 0;
	font-family: "Trebuchet MS", Tahoma, Verdana;
	background: url(../images/title_bg2.png)  no-repeat #dbdbdb;
}

.box_top {
	margin: 35px 0 0 30px;
}

.box {
	margin: 0 0 0 30px;
}
.box h1 , .box_top h1{
	text-align:center center;
	font-size: 16px;
	font-variant: small-caps;
	color: #fff;
	font-weight: bold;
	margin: 0 0 15px 20px;
}


