ul.botLink{
	height:32px;
	background:url(../images/head_bot_bg.gif) 0 0 repeat-y;
	float:left;
	margin: 0 0 0 100px;
	padding:0 0 0 29px;
	
}

ul.botLink li{
	float:left;
	margin:0 54px 0 -20px;
	display:block;
	min-width: 100px;
}

ul.botLink li.noMargin2{
	margin:0;
}

ul.botLink li {
	font:bold 12px/32px Arial, Helvetica, sans-serif;
	color:#FAFDFF;
	/*background-color:inherit;*/
	text-decoration:none;
}

ul.botLink li:hover{
	color:#DB0C90;
	background-color:inherit;
}


#content_top {
	width: 100%;
	/*background:url(../images/content_area_top.jpg) #fff   no-repeat;*/
	background:#FFFFFF;
	margin: 1px 0 0 0;
	padding: 0 0 20px 0;
}


#content_bottom {
	width: 100%;
	/*background:url(../images/content_area_bottom.jpg) #fff   no-repeat;*/
	margin: 0;
	padding: 0 0 19px 0;
}
/*------------------------------------------------foot---------------------*/

#footer {
	padding: 0;
	margin:0;
	font-family: Arial;
	font-weight: bold;
	font-size: 12px;
	text-align: center;
	
}


.cleaner {
	clear: both;
	height: 0;
	margin: 0;
	padding: 0;
}
