/* CSS Document */


#body {
	padding: 0px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 1.5em;
	width: 100%;
	display: table;
	background: url(../images/body_bg.jpg) repeat-x;
	text-align:center;
	margin:0 auto;
}

a:link, a:visited {	COLOR: #0099FF; text-decoration: underline; }
a:active, a:hover { color: #0000ff; text-decoration: none; }

/*------------------------------------------------body--------------------*/

/*------------------------------------------------main--------------------*/
#my_container{
	width: 100%;
	margin: auto;
}


/*------------------------------------------------top---------------------*/
#header {
	width: 887px;
	height: 68px;
	background: url(../images/header_bg.jpg);
	margin-top:1px;
}

#logo {
	float: left;
	width: 272px;
	height: 59px;
	background: url(../images/logo_bg.jpg);
	margin: 8px 0 0 40px;
	font-family: "Trebuchet MS", Tahoma, Verdana;
}

#logo h1 {
	color: #b3d46a;
	font-size: 28px;
	margin: 15px 0 0 30px;
	padding: 0;
}

#logo h2 {
	color: #98e11d;
	font-size: 14px;
	margin: 10px 0 0 55px;
	padding: 0;
}

/* menu */
#menu {
	float: right;
	height: 41px;
	width: 560px;
	margin: 5px 0 0 0;
	padding: 0;
}

#menu ul {
	clear: both;
	float: right;
	list-style: none;
	display: inline;
}

#menu li {
	float: left;
	font-size: 22px;
}

#menu li a {
	float: left;
	display: block;
	color: #fff;
	text-decoration: none;
	font-family: Tahoma;
	
	margin: 0 10px 0 0; 
	padding: 0 0 0 5px;
	height: 41px;
	line-height: 33px;
	text-align: center;
	cursor: pointer;
}

#menu li a b {
	float: left;
	display: block;
	height: 38px;
	padding: 3px 15px 0 11px;
}

#menu li.current a, #menu li a:hover {
	background: url(../images/menu_left.png) no-repeat;
	color: #fff;
}

#menu li.current a b, #menu li a:hover b {
	background: url(../images/menu_right.png) right top no-repeat;
}

/*End OF menu*/


/*--------------------------------------left & right---------------------------------*/
#left{
	width: 14%;
	padding-left: 0px;
	margin-left: 0px;
	padding-right: 0px;
	margin-right: 0px;
	float:left;
	font-size:15px;
	border:1px solid black;
	height:554px;
	
}

#treeList {
	
	font-family: "Times New Roman", Georgia, Serif;
	font-size: 16px;
	border-collapse: collapse;
	text-align:center;
	width:96%;
	margin:35px auto;
	vertical-align:middle;
}

#treeList tr {
	display: table-row;
	vertical-align: inherit;
	border-color: inherit;
}

#treeList tbody tr:hover {
	background-color:#e8edff;
}

#treeList td {
	padding: 9px 8px 0;
}

#treeList td, #treeList th {
	display: table-cell;
	vertical-align: inherit;
}

/*-----right----------*/
#right{
	border-left:1px #800000;
	float:right;
	width: 85%;
	padding-left: 0px;
	padding-right: 0px;
	border:1px solid black;
	height:554px;
}
