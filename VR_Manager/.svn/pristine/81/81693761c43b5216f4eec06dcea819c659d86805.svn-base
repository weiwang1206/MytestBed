		loadXML1();
		table_autoIncrease();
		}

	function table_autoIncrease()
	{
		var oTable = document.getElementById("VRTable");
		for(var i=0;i<oTable.rows.length;i++){
			oTable.rows[i].cells[0].innerHTML = (i+1);
			}
	}

	
	
	
	
